<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="unknow" id="1">

        <loadData catalogName="catalogzapchasty"
                  commentLineStartsWith="A String"
                  encoding="UTF-8"
                  file="../src/main/resources/db/changelog/files_for_db/vehicles.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="public"
                  separator=","
                  tableName="vehicles"
                  usePreparedStatements="true" >
            <column header="carid" name="carid" type="integer"/>
            <column header="brand" name="brand" type="text"/>
        </loadData>

    </changeSet>
</databaseChangeLog>