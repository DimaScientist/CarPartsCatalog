<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="unknow" id="1">

        <loadData tableName="country" encoding="UTF-8"
                  file="src/main/resources/db/changelogs/country.csv"
                  quotchar="'" separator=";" >
            <column header="countryid" name="login" type="STRING"/>
            <column header="oid" name="oid" type="STRING"/>
            <column header="nombre" name="nombre" type="STRING"/>
            <column header="apellidos" name="apellidos" type="STRING"/>
            <column header="activo" name="activo" type="BOOLEAN"/>
        </loadData>
    </changeSet>
</databaseChangeLog>