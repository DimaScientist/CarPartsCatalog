<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="unknow" id="1">

        <loadData catalogName="catalogavtozapchasti"
                  commentLineStartsWith="A String"
                  encoding="UTF-8"
                  file="src/main/recources/db/changelog/parts.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="public"
                  separator=","
                  tableName="parts"
                  usePreparedStatements="true">
            <column header="partnum" name="partnum" type="text"/>
            <column header="brantype" name="brantype" type="text"/>
            <column header="faltpartnum" name="faltpartnum" type="text"/>
            <column header="brand" name="brand" type="text"/>
            <column header="descr" name="descr" type="text"/>
            <column header="arstat" name="arstat" type="text"/>
        </loadData>

    </changeSet>
</databaseChangeLog>