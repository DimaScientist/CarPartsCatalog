<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="unknow" id="1">

        <loadData catalogName="catalogavtozapchasti"
                  commentLineStartsWith="A String"
                  encoding="UTF-8"
                  file="src/main/recources/db/changelog/price.csv"
                  quotchar="'"
                  relativeToChangelogFile="true"
                  schemaName="public"
                  separator=","
                  tableName="price"
                  usePreparedStatements="true">
            <column header="countryid" name="countryid" type="integer"/>
            <column header="rrp" name="rrp" type="numeric"/>
            <column header="pridate" name="pridate" type="date"/>
            <column header="partnum" name="partnum" type="text"/>
        </loadData>

    </changeSet>
</databaseChangeLog>